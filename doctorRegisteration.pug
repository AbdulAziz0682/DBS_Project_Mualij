doctype html
head
  title Health
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
  meta(name='description' content='Free HTML5 Website Template by uicookies.com')
  meta(name='keywords' content='free bootstrap 4, free bootstrap 4 template, free website templates, free html5, free template, free website template, html5, css3, mobile first, responsive')
  meta(name='author' content='uicookies.com')
  link(href='https://fonts.googleapis.com/css?family=Work+Sans' rel='stylesheet')
  link(rel='stylesheet' href='css/bootstrap.min.css')
  link(rel='stylesheet' href='css/open-iconic-bootstrap.min.css')
  link(rel='stylesheet' href='css/owl.carousel.min.css')
  link(rel='stylesheet' href='css/owl.theme.default.min.css')
  link(rel='stylesheet' href='css/icomoon.css')
  link(rel='stylesheet' href='css/flaticon.css')
  link(rel='stylesheet' href='css/animate.css')
  link(rel='stylesheet' href='css/bootstrap-datepicker.css')
  link(rel='stylesheet' href='css/style.css')
nav.navbar.navbar-expand-lg.navbar-dark.bg-dark.probootstrap-navbar-dark
  .container
    button.navbar-toggler(type='button' data-toggle='collapse' data-target='#probootstrap-nav' aria-controls='probootstrap-nav' aria-expanded='false' aria-label='Toggle navigation')
      span.navbar-toggler-icon
    #probootstrap-nav.collapse.navbar-collapse
      ul.navbar-nav.mr-auto
        li.nav-item.active
          a.nav-link.pl-0(href='index.html') Home
        li.nav-item
          a.nav-link(href='departments.html') Departments
        li.nav-item
          a.nav-link(href='about.html') About
        li.nav-item
          a.nav-link(href='contact.html') Contact
        li.nav-item
          a.nav-link(href='login.html') Login
      .ml-auto
        form.probootstrap-search-form.mb-sm-0.mb-3(action='#' method='get')
          .form-group
            button.icon.submit
              span.icon-magnifying-glass
            input.form-control(type='text' placeholder='Search')
// END nav
header.probootstrap-header.py-5(role='banner')
  .container
    .row
      .col-md-3.mb-4
        a.mr-auto(href='index.html')
          img.hires(src='images/logo.png' width='212' height='48' alt='Free Template by uiCookies')
      .col-md-9
        .float-md-right.float-none
          .probootstrap-contact-phone.d-flex.align-items-top.mb-3.float-left
            span.icon.mr-2
              i.icon-phone
            span.probootstrap-text
              | +900 123 456 7 
              small.d-block
                a.arrow-link(href='#')
                  | Appointment 
                  i.icon-chevron-right
section.probootstrap-services.login-section(style="background-color: #32609e")
  .col-md-6.p-md-5.p-3.probootstrap-aside-stretch-right(style='text-align: center; margin: 0 auto')
    h2.h1.text-white(style='width: 100%;') Register as a Doctor
    div#message
      span#errorMessage(style="color:lightcoral; font-weight: bold;") #{errorMessage ? errorMessage : ''}
      span#successMessage(style="color: lightgreen; font-weight: bold;") #{successMessage ? successMessage : ''}
    form.probootstrap-form-appointment(action='/doctorRegisteration' method='POST')
      .form-group
        input.form-control(name='firstName' type='text' placeholder='First Name' required='')
      .form-group
        input.form-control(name='lastName' type='text' placeholder='Last Name' required='')
      .form-group
        input.form-control(name='email' type='text' placeholder='Email Address' required='')
      .form-group
        input.form-control(name='password' type='password' placeholder='Password' required='')
      .form-group
        input.form-control(name='confirmPassword' type='password' placeholder='Confirm Password' required='')
      .form-group
        input.form-control(name='department' type='text' placeholder='Department' required='')
      .form-group
        .input-group-prepend
          span.input-group-text Start Time
        input#startTime.form-control(name='startTime' type='time' required='')
      .form-group
        .input-group-prepend
          span.input-group-text End Time
        input.form-control(name='endTime' type='time' required='')
      .form-group
        .input-group-prepend
          span.input-group-text Week Days
        label(for='mon') MON
        input#mon(type='checkbox' name='MON')
        label(for='tue') TUE
        input#tue(type='checkbox' name='TUE')
        label(for='wed') WED
        input#wed(type='checkbox' name='WED')
        label(for='thu') THU
        input#thu(type='checkbox' name='THU')
        label(for='fri') FRI
        input#fri(type='checkbox' name='FRI')
        label(for='sat') SAT
        input#sat(type='checkbox' name='SAT')
        label(for='sun') SUN
        input#sun(type='checkbox' name='SUN')
      .form-group
        input.form-control(name='age' type='number' placeholder='Age' required='')
      .form-group
        input.form-control(name='gender' type='text' placeholder='Gender' required='')
      .form-group
        input.form-control(name='address' type='text' placeholder='Address' required='')
      .row.justify-content-center.mb-5
        // <div class="col-md-4"><a href="#" class="btn btn-secondary btn-block">Login <span class="icon-arrow-right"></span></a></div>
        .col-md-4
          button.btn.btn-secondary.btn-block(type='submit') Register
      .col-md-12 
          a(href="./login.html" style="font-size: small") Already have an account? Login Now!
